}

@model Pharmscy_Management.Models.Tablet

    <html>
    <head>

        <title>Tablet</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">


        <link href="https://fonts.googleapis.com/css?family=Rubik:400,700|Crimson+Text:400,400i" rel="stylesheet">
        <link rel="stylesheet" href="~/Design/fonts/icomoon/style.css">

        <link rel="stylesheet" href="~/Design/css/bootstrap.min.css">
        <link rel="stylesheet" href="~/Design/css/magnific-popup.css">
        <link rel="stylesheet" href="~/Design/css/jquery-ui.css">
        <link rel="stylesheet" href="~/Design/css/owl.carousel.min.css">
        <link rel="stylesheet" href="~/Design/css/owl.theme.default.min.css">
        <link rel="stylesheet" href="~/Design/css/aos.css">
        <link rel="stylesheet" href="~/Design/css/style.css">

        @*sidebar links*@
        <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">

        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="~/Design/sidebar/css/style.css">
    </head>
    <body style="width:auto">

        <div class="wrapper d-flex align-items-stretch">
            <nav id="sidebar" class="">
                <h1><a href="index.html" class="logo">Pharmacy</a></h1>
                <ul class="list-unstyled components mb-auto">
                    <li class="active">
                        <a href="@Url.Action("Admin_Home","Admin")"><span class="fa fa-home"></span>Home</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","Home")"><i class="fa fa-desktop "></i> MY Web</a>
                    </li>

                    <li class="dropdown">
                        <a href="#"><i class="fa fa-cart-arrow-down"></i> Products</a>
                        <ul class="dropdown">

                            <li>
                                <a href="@Url.Action("Index","Tablets")">Tablets<span class="fa arrow"></span></a>

                            </li>

                            <li>
                                <a href="@Url.Action("Index","Capsules")">Capsules<span class="fa arrow"></span></a>

                            </li>
                            <li>
                                <a href="@Url.Action("Index","Liquid_fillings")">Liquid Fillings<span class="fa arrow"></span></a>

                            </li>
                        </ul>
                    </li>

                    <li>
                        <a href="@Url.Action("career","Admin")"><i class="fa fa-book "></i> Career</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Qoute","Admin")"><i class="fa fa-table"></i> Quote</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Contact","Admin")"><i class="fa fa-table "></i> Contact</a>
                    </li>

                    <li>
                        <a href="@Url.Action("Admin_SignIn","Admin")"><i class="fa fa-sign-in"></i> Login</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Admin_SignUp","Admin")"><i class="fa fa-registered"></i> Registeration</a>
                    </li>

                </ul>

            </nav>

            <!-- Page Content  -->
            <div id="content" class="p-4 p-md-5">

                <nav class="navbar navbar-expand-lg navbar-light bg-light">
                    <div class="container-fluid">

                        <button type="button" id="sidebarCollapse" class="btn btn-primary">
                            <i class="fa fa-bars"></i>
                            <span class="sr-only">Toggle Menu</span>
                        </button>
                        <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <i class="fa fa-bars"></i>
                        </button>

                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="nav navbar-nav ml-auto">
                                <li class="nav-item active">
                                    <a class="nav-link" href="@Url.Action("Admin_Home","Adnin")">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="@Url.Action("Index","Home")">My Web</a>
                                </li>
                                <li class="nav-item btn-outline-primary">
                                    <a class="nav-link" href="@Url.Action("Admin_SignIn","Admin")">Log Out</a>
                                </li>

                            </ul>
                        </div>
                    </div>
                </nav>

                <div class="row mb-auto">
                    <div class="col-md-12">
                        <!-- Advanced Tables -->
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                Tablets Details
                            </div>
                            <div class="panel-body sm">
                                <div class="table-responsive-sm" style="width:auto">

                                    <h3>Are you sure you want to delete this?</h3>
                                    <div>
                                        <h4>Tablet</h4>
                                        <hr />
                                        <dl class="dl-horizontal">
                                            <dt>
                                                @Html.DisplayNameFor(model => model.Tb_Name)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Tb_Name)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Model_No)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Model_No)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Dies)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Dies)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Max_Pressure)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Max_Pressure)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Max_Diametere)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Max_Diametere)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Max_Depth)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Max_Depth)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Production_Capacity)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Production_Capacity)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Machine_Size)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Machine_Size)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.Net_Weight)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.Net_Weight)
                                            </dd>

                                            <dt>
                                                @Html.DisplayNameFor(model => model.tablet_Image)
                                            </dt>

                                            <dd>
                                                @Html.DisplayFor(model => model.tablet_Image)
                                            </dd>

                                        </dl>

                                        @using (Html.BeginForm())
                                        {
                                            @Html.AntiForgeryToken()

                                            <div class="form-actions no-color">
                                                <input type="submit" value="Delete" class="btn btn-default" /> |
                                                @Html.ActionLink("Back to List", "Index")
                                            </div>
                                        }
                                    </div>
                                </div>

                            </div>
                        </div>
                        <!--End Advanced Tables -->
                    </div>
                </div>


            </div>
        </div>





        <script src="~/Design/js/jquery-3.3.1.min.js"></script>
        <script src="~/Design/js/jquery-ui.js"></script>
        <script src="~/Design/js/popper.min.js"></script>
        <script src="~/Design/js/bootstrap.min.js"></script>
        <script src="~/Design/js/owl.carousel.min.js"></script>
        <script src="~/Design/js/jquery.magnific-popup.min.js"></script>
        <script src="~/Design/js/aos.js"></script>
        <script src="~/Design/js/main.js"></script>
        @*sidebar links*@
        <script src="~/Design/sidebar/js/jquery.min.js"></script>
        <script src="~/Design/sidebar/js/popper.js"></script>
